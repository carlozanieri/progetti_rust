-- Table: submenu
CREATE TABLE IF NOT EXISTS submenu(
  id INT,
  codice TEXT,
  radice TEXT,
  livello INT,
  titolo TEXT,
  descrizione TEXT,
  link TEXT,
  attivo INT,
  icon TEXT,
  workplace INT,
  param1 TEXT,
  ordine INT
);
